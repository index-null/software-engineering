            "type": "object",
            "properties": {
                "id": {
                    "type": "integer"
                },
                "url": {
                    "type": "string"
                }
            }
        },
        "generate_s.ImageParaments": {
            "type": "object",
            "required": [
                "height",
                "prompt",
                "sampling_method",
                "seed",
                "steps",
                "width"
            ],
            "properties": {
                "height": {
                    "type": "integer",
                    "maximum": 1024,
                    "minimum": 128
                },
                "prompt": {
                    "type": "string"
                },
                "sampling_method": {
                    "type": "string",
                    "enum": [
                        "DDIM",
                        "PLMS",
                        "K-LMS"
                    ]
                },
                "seed": {
                    "type": "string"
                },
                "steps": {
                    "type": "integer",
                    "maximum": 100,
                    "minimum": 1
                },
                "width": {
                    "type": "integer",
                    "maximum": 1024,
                    "minimum": 128
                }
            }
        },
